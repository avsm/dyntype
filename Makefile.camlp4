OCAMLMAKEFILE=OCamlMakefile

ANNOTATE=Yes
USE_CAMLP4=Yes
SOURCES= error.ml types.ml parse.ml pa_sql_orm.ml
LIB_PACK_NAME = orm
PACKS = type-conv
RESULT = pa_sql_orm


.PHONY: p-%
p-%: pa_sql_orm.cma
	camlp4orf -I`ocamlc -where`/site-lib/type-conv pa_type_conv.cmo -parser sql_orm.cma pa_sql_orm.cma $*.ml

r-%: pa_sql_orm.cma
	ocamlfind ocamlc -o $@ -pp "camlp4orf -I `ocamlfind query type-conv` sql_orm.cma pa_type_conv.cmo pa_sql_orm.cma " $*.ml

include $(OCAMLMAKEFILE)
